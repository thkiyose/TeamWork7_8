<p id="notice"><%= notice %></p>
<h1>登録内容変更</h1>
<%= form_with(model: @user, local: true) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :email %>
<%= f.text_field :email %>
